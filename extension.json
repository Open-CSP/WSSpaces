{
  "name": "WSSpaces",
  "version": "1.0",
  "namemsg": "wss-extensionname",
  "url": "https://wikibase-solutions.com",
  "type": "other",
  "author": [
    "[https://mediawiki.org/wiki/User:Xxmarijnw Marijn van Wezel]"
  ],
  "descriptionmsg": "wss-desc",
  "license-name": "MIT",
  "requires": {
    "MediaWiki": ">= 1.31.0",
    "extensions": {
      "Lockdown": "*"
    }
  },
  "MessagesDirs": {
    "WSSpaces": [
      "i18n"
    ]
  },
  "ExtensionMessagesFiles": {
    "WSSpacesMagic": "i18n/WSSpaces.i18n.php"
  },
  "AutoloadNamespaces": {
    "WSS\\": "src/"
  },
  "Hooks": {
    "SecuritySensitiveOperationStatus": "WSS\\WSSHooks::onSecuritySensitiveOperationStatus",
    "SkinBuildSidebar": "WSS\\WSSHooks::onSkinBuildSidebar",
    "LoadExtensionSchemaUpdates": "WSS\\WSSHooks::onLoadExtensionSchemaUpdates",
    "BeforeInitialize": "WSS\\WSSHooks::onBeforeInitialize",
    "CanonicalNamespaces": "WSS\\WSSHooks::onCanonicalNamespaces"
  },
  "SpecialPages": {
    "Permissions": "WSS\\Special\\SpecialPermissions",
    "AddSpace": "WSS\\Special\\SpecialAddSpace",
    "ManageSpace": "WSS\\Special\\SpecialManageSpace",
    "ArchivedSpaces": "WSS\\Special\\SpecialArchivedSpaces"
  },
  "AvailableRights": [
    "wss-manage",
    "wss-edit-core-namespaces",
    "wss-edit-all-spaces"
  ],
  "GroupPermissions": {
    "sysop": {
      "wss-manage": true,
      "wss-edit-core-namespaces": true,
      "wss-edit-all-spaces": false
    },
    "spacemaster": {
      "wss-manage": true,
      "wss-edit-all-spaces": true
    }
  },
  "ResourceModules": {
    "ext.wss.SpecialPermissions": {
      "styles": "extensions/WSSpaces/resources/styles/ext.wss.SpecialPermissions.css"
    },
    "ext.wss.Spaces": {
      "styles": "extensions/WSSpaces/resources/styles/ext.wss.Spaces.css"
    },
    "ext.wss.Global": {
      "styles": "extensions/WSSpaces/resources/styles/ext.wss.Global.css"
    },
    "ext.wss.Exception": {
      "styles": "extensions/WSSpaces/resources/styles/ext.wss.Exception.css"
    },
    "ext.wss.SpecialPermissionsSuccess": {
      "scripts": "extensions/WSSpaces/resources/scripts/ext.wss.SpecialPermissionsSuccess.js",
      "position": "bottom",
      "messages": [
        "wss-permissions-saved-successfully"
      ],
      "dependencies": [
        "mediawiki.util",
        "mediawiki.api"
      ]
    },
    "ext.wss.AddSpaceSuccess": {
      "scripts": "extensions/WSSpaces/resources/scripts/ext.wss.AddSpaceSuccess.js",
      "position": "bottom",
      "messages": [
        "wss-add-space-saved-successfully"
      ],
      "dependencies": [
        "mediawiki.util",
        "mediawiki.api"
      ]
    },
    "ext.wss.ArchiveSpaceSuccess": {
      "scripts": "extensions/WSSpaces/resources/scripts/ext.wss.ArchiveSpaceSuccess.js",
      "position": "bottom",
      "messages": [
        "wss-archive-space-saved-successfully"
      ],
      "dependencies": [
        "mediawiki.util",
        "mediawiki.api"
      ]
    },
    "ext.wss.UnarchiveSpaceSuccess": {
      "scripts": "extensions/WSSpaces/resources/scripts/ext.wss.UnarchiveSpaceSuccess.js",
      "position": "bottom",
      "messages": [
        "wss-unarchive-space-saved-successfully"
      ],
      "dependencies": [
        "mediawiki.util",
        "mediawiki.api"
      ]
    },
    "ext.wss.SpecialManageSpaceSuccess": {
      "scripts": "extensions/WSSpaces/resources/scripts/ext.wss.SpecialManageSpaceSuccess.js",
      "position": "bottom",
      "messages": [
        "wss-manage-space-saved-successfully"
      ],
      "dependencies": [
        "mediawiki.util",
        "mediawiki.api"
      ]
    }
  },
  "config": {
    "WSSValidRights": {
      "value": [
        "read",
        "edit",
        "delete",
        "move",
        "protect"
      ]
    },
    "WSSValidUserGroups": {
      "value": []
    },
    "WSSValidNamespaces": {
      "value": ["Main", "Media", "User", "Project", "File", "MediaWiki", "Template", "Help", "Category"]
    }
  },
  "LogTypes": [
    "space"
  ],
  "LogActionsHandlers": {
    "space/*": "LogFormatter"
  },
  "manifest_version": 2
}